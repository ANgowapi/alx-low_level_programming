#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/**
* main - print the last digit of the number stored
*
* Description: program will assign a random number to
* variable n each time it is executed.
*
* Return: Always 0 (Succes)
*/
int main(void)
{
int n; last;

srand(time(0));
n = rand() - RAND_MAX / 2;
last = n % 10;
if (last > 5)
printf("Last digit of %d is %d and is greater than 5\n", n, last);
else if (last == 0)
printf("Last digit of %d is %d and is 0\n", n, last);
else if (last < 6)
printf("Last digit of %d is %d and is less than 6\n", n, last);
else if (last != 0)
printf("Last digit of %d is %d and is not 0\n", n, last);
return (0);
}

#include <stdio.h>

/**
* main - print single digit numbers
*
* Description: prints all single digit numbers of base 10 starting from 0
*
* Return: Always 0 (Success)
*/

int main(void)
{
int num;

for (num = 0)
printf("%d\n", num);
for (num < 10)
printf("%d\n", num);
for (num++);
printf("%d\n", num);
return (0);
}
